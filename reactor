end

function Restaurar()

    if savedValues == nil then

        gg.alert("‚ö†Ô∏èNo se pudo restaurar, falta de datos, por favor reinicia el juego.")

        return

    end



    for i, result in ipairs(savedValues) do

        gg.setValues(result)

        gg.clearResults()

    end



    gg.toast("Restaurados.")

end

